{
  "private": true,
  "scripts": {
    "dev": "concurrently -n API,WEB -c green, cyan \"npm:dev:api\" \"npm:dev:web\"",
    "dev:api": "npm --prefix api run dev",
    "dev:web": "npm --prefix web run dev",
    "build": "npm --prefix api run build && npm --prefix web run build"
  },
  "devDependencies": {
    "concurrently": "^9.2.0"
  }
}
